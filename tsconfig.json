{
  "compilerOptions": {
    "outDir": "./dist",
    "sourceMap": true,
    "module": "esNext",
    "target": "es5",
    "jsx": "preserve",
    "allowJs": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "dom"
    ],
    //"forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    //"noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "declaration": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "src/**/*",
    "custom.d.ts"
  ],
  "exclude": [
  ]
}
